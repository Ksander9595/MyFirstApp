@model IEnumerable<MyFirstApp.Models.User>

<h2>Users List</h2>
<p><a asp-action="Create">Add User</a></p>
<table class="table">
    <tr><th>Name</th><th>Age</th><th></th><th></th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Change</a>
            </td>
            <td>
                <form asp-controller="Home" asp-action="Delete" method="post" asp-route-id="@item.Id">
                    <input type ="submit" value="Delete" />
                </form>
            </td>
        </tr>
    }
</table>